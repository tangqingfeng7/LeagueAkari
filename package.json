    "axios": "^1.10.0",
    "axios-retry": "^4.5.0",
    "dayjs": "^1.11.13",
    "fast-csv": "^5.0.2",
    "lodash": "^4.17.21",
    "luaparse": "^0.3.1",
    "mobx": "^6.13.7",
    "node-7z": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "regedit": "^5.1.4",
    "sqlite3": "5.1.7",
    "typeorm": "^0.3.25",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^3.0.1",
