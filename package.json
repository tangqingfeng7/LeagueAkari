    "@swc/core": "^1.12.9",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash": "^4.17.20",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.0.10",
    "@types/node-7z": "^2.1.10",
    "@types/semver": "^7.7.0",
